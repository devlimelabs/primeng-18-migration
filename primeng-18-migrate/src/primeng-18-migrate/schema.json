{
  "$schema": "http://json-schema.org/schema",
  "id": "PrimeNGMigrationSchematic",
  "title": "PrimeNG v18 Migration Options",
  "type": "object",
  "properties": {
    "skipGitCheck": {
      "type": "boolean",
      "description": "Skip checking for unstaged Git changes",
